Xcursor.theme: Paper

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

Emacs.font: Go Mono:size=12
XTerm*font: xft:Go Mono:size=9

*.foreground: #5f5f5f
*.background: #fdfde7
*.color0:     #3a3a3a
*.color8:     #8a8a8a
*.color1:     #cc0000
*.color9:     #ef2929
*.color2:     #5fd700
*.color10:    #d7ff87
