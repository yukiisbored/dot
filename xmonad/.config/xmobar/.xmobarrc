Config
  { font              = "xft:Fira Sans, Migu 1P:size=12"
  , additionalFonts   = [ "xft:FontAwesome:pixelsize=14:antialias=true:hinting=true" ]
  , border            = NoBorder
  , bgColor           = "#000000"
  , fgColor           = "#aaaaaa"
  , alpha             = 200
  , position          = Top
  , commands          =
      [ Run Cpu [ "-t","<fn=1>\xf085</fn> <fc=#ffffff><total>%</fc>" ] 10
      , Run Memory ["-t","<fn=1>\xf2db</fn> <fc=#ffffff><usedratio>%</fc>"] 10
      , Run BatteryP ["BAT0"]
                     [ "-t", "<acstatus> <fc=#ffffff><left>%</fc>"
                     , "--"
                     , "-O", "<fn=1>\xf1e6</fn>"
                     , "-i", "<fn=1><fc=green>\xf1e6</fc></fn>"
                     , "-o", "<fn=1>\xf240</fn>" ]
                     10
      , Run Date "<fn=1>\xf017</fn> <fc=#ffffff>%a, %d %b %H:%M</fc>" "date" 10
      , Run Wireless "wlan0" ["-t", "<fn=1>\xf1eb</fn> <fc=#ffffff><ssid></fc>"] 10
      , Run Locks
      , Run StdinReader
      ]
  , sepChar           = "%"
  , alignSep          = "}{"
  , template          = "  %StdinReader% }{ %locks%  %wlan0wi%  %cpu%  %memory%  %battery%  %date%  "
  }
